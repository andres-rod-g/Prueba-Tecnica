{"ast":null,"code":"import \"vue-select/dist/vue-select.css\";\nexport default {\n  name: \"FormProduct\",\n  props: ['product_prop', 'products_fetched', 'selected_currency_info'],\n  components: {},\n  methods: {\n    addUnit(e) {\n      e.preventDefault();\n      this.units += 1;\n      this.quantity_price = this.currency + ' ' + this.units * this.currency_price;\n    },\n\n    decreseUnit(e) {\n      e.preventDefault();\n\n      if (this.units > 1) {\n        this.units -= 1;\n      } else {\n        //Remove Product\n        console.log('about to remove');\n        this.$emit('removeProduct', this.product_prop.entry_uuid);\n      }\n\n      this.quantity_price = this.currency + ' ' + this.units * this.currency_price;\n    }\n\n  },\n\n  data() {\n    return {\n      units: 1,\n      discount: 0,\n      currency: 'USD',\n      currency_price: 0,\n      conversion: 0,\n      quantity_price: 0,\n      selectedProduct: {\n        \"sku\": \"--\",\n        \"name\": \"Pinzas\",\n        \"price\": 0,\n        \"currency\": \"USD\",\n        \"taxRate\": 0\n      }\n    };\n  },\n\n  watch: {\n    selected_currency_info: {\n      immediate: true,\n\n      handler(val) {\n        this.currency_price = val.value;\n        console.log(this.currency, val.code);\n\n        if (this.currency == val.code) {\n          this.quantity_price = this.currency + ' ' + this.units * this.currency_price;\n        }\n      }\n\n    }\n  }\n};","map":{"version":3,"mappings":"AAyBA,OAAO,gCAAP;AAGA,eAAe;AACXA,MAAI,EAAE,aADK;AAEXC,OAAK,EAAE,CACH,cADG,EAEH,kBAFG,EAGH,wBAHG,CAFI;AAOXC,YAAU,EAAE,EAPD;AASXC,SAAO,EAAE;AACLC,WAAO,CAACC,CAAD,EAAI;AACPA,OAAC,CAACC,cAAF;AACA,WAAKC,KAAL,IAAc,CAAd;AAEA,WAAKC,cAAL,GAAsB,KAAKC,QAAL,GAAgB,GAAhB,GAAuB,KAAKF,KAAL,GAAa,KAAKG,cAA/D;AACH,KANI;;AAOLC,eAAW,CAACN,CAAD,EAAI;AACXA,OAAC,CAACC,cAAF;;AAEA,UAAI,KAAKC,KAAL,GAAa,CAAjB,EAAoB;AAChB,aAAKA,KAAL,IAAc,CAAd;AACJ,OAFA,MAEO;AACH;AACAK,eAAO,CAACC,GAAR,CAAY,iBAAZ;AACA,aAAKC,KAAL,CAAW,eAAX,EAA4B,KAAKC,YAAL,CAAkBC,